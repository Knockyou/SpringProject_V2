{{>layouts/header}}

<!-- contents -->
<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col">Major</th>
        <th scope="col">Age</th>
        <th scope="col">Grade</th>
        <th scope="col">Kisa</th>
        <th scope="col">Job</th>
    </tr>
    </thead>
    <tbody>
    {{#students}}
        <tr>
            <th>{{id}}</th>
            <td>{{name}}</td>
            <td>{{major}}</td>
            <td>{{age}}</td>
            <td>{{grade}}</td>
            <td>{{kisa}}</td>
            <td>{{job}}</td>
        </tr>
    {{/students}}
    </tbody>
</table>

{{>layouts/footer}}